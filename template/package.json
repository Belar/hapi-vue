{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "author": "{{ author }}",
  {{#private}}
  "private": true,
  {{/private}}
  "scripts": {
    "dev": "gulp",
    {{#unit}}
    "unit": "karma start test/unit/karma.conf.js",
    {{/unit}}
    {{#unitApi}}
    "unit-api": "lab test/api/spec/ -C -v -c -r console -o stdout -r html -o test/api/coverage/index.html --coverage-exclude .eslintrc.js",
    {{/unitApi}}
    "build": "webpack --progress --hide-modules --config config/production.webpack.config.js"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "hapi": "^16.0.1",
    "inert": "^4.0.0",
    "vue": "^2.1.0",
    "vue-resource": "^1.0.3"
  },
  "devDependencies": {
    "browser-sync": "^2.17.3",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.1.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "hapi-webpack-dev-middleware": "^2.0.0",
    "hapi-webpack-hot-middleware": "^2.0.0",
    "html-webpack-plugin": "^2.24.0",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^10.0.1",
    "vue-template-compiler": "^2.1.3",
    "vue-style-loader": "^1.0.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "url-loader": "^0.5.7",
    "function-bind": "^1.1.0",
    "node-sass": "^4.0.0",
    "sass-loader": "^4.0.2",
    {{#lint}}
    "eslint": "^3.8.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    {{/lint}}
    {{#if_or unit unitApi}}
    "chai": "^3.5.0",
    {{/if_or}}
    {{#unit}}
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-sinon-stub-promise": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lolex": "^1.4.0",
    "mocha": "^3.1.0",
    "phantomjs-prebuilt": "^2.1.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "sinon-stub-promise": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "inject-loader": "^2.0.1",
    {{/unit}}
    {{#unitApi}}
    "lab": "^11.2.1",
    {{/unitApi}}
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-hot-middleware": "^2.14.0",
    "webpack-merge": "^2.0.0",
    "webpack-merge": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4"
  }
}
