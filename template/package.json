{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "author": "{{ author }}",
  {{#private}}
  "private": true,
  {{/private}}
  "scripts": {
    "dev": "gulp",
    {{#unit}}
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js",
    {{/unit}}
    {{#unitApi}}
    "unit-api": "lab test/api/spec/ -C -v -c -r console -o stdout -r html -o test/api/coverage/index.html --coverage-exclude .eslintrc.js",
    {{/unitApi}}
    "build": "webpack --progress --hide-modules --config config/production.webpack.config.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.23.0",
    "hapi": "^17.1.1",
    "inert": "^5.0.1",
    "vue": "^2.3.4"
  },
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "cross-env": "^5.0.1",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "html-webpack-plugin": "^2.29.0",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.0",
    "vue-template-compiler": "^2.3.4",
    "vue-style-loader": "^3.0.0",
    "css-loader": "^0.28.0",
    "file-loader": "^1.1.6",
    "url-loader": "^0.6.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    {{#lint}}
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    {{/lint}}
    {{#if_or unit unitApi}}
    "chai": "^4.0.2",
    {{/if_or}}
    {{#unit}}
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sinon-stub-promise": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.3",
    "lolex": "^2.1.2",
    "mocha": "^4.0.1",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.11.0",
    "sinon-stub-promise": "^4.0.0",
    "inject-loader": "^3.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    {{#if_eq headlessBrowser "PhantomJS"}}
    "function-bind": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-prebuilt": "^2.1.14",
    {{/if_eq}}
    {{#if_eq_or headlessBrowser "ChromeHeadless" "ChromiumHeadless"}}
    "karma-chrome-launcher": "^2.2.0",
    {{/if_eq_or}}
    {{#if_eq headlessBrowser "FirefoxHeadless"}}
    "karma-firefox-launcher": "^1.1.0",
    {{/if_eq}}
    {{/unit}}
    {{#unitApi}}
    "lab": "^15.1.2",
    {{/unitApi}}
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.0"
  }
}
